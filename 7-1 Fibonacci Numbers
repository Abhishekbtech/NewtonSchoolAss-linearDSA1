static long  Fibonacci(long  n) 
    { 
        //Enter your code here
        if (n == 1) {
            return n;
        }

        long a = 0;
        long b = 1;
        long c;

        for (int i = 2; i <= n; i++) {
            c = a + b;
            a = b;
            b = c;
        }

        return b;
    }
